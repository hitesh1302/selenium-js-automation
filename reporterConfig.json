{
  "reporterEnabled": "mochawesome, mocha-junit-reporter",
  "mochawesomeReporterOptions": {
    "reportDir": "mochawesome-report",
    "reportFilename": "report",
    "quiet": true,
    "overwrite": false,
    "html": false,
    "json": true
  },
  "mochaJunitReporterReporterOptions": {
    "mochaFile": "./test-results/results.xml",
    "toConsole": true
  }
}

